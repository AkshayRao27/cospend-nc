<?xml version="1.0"?>
<info>
    <id>cospend</id>
    <name>Cospend</name>
    <summary> </summary><description> </description>
    <version>0.5.1</version>
    <licence>agpl</licence>
    <author mail="eneiluj@posteo.net">Julien Veyssier</author>
    <namespace>Cospend</namespace>
    <documentation>
        <user>https://gitlab.com/eneiluj/cospend-nc/wikis/userdoc</user>
        <admin>https://gitlab.com/eneiluj/cospend-nc/wikis/admindoc</admin>
        <developer>https://gitlab.com/eneiluj/cospend-nc/wikis/devdoc</developer>
    </documentation>
    <category>tools</category>
    <category>organization</category>
    <website>https://gitlab.com/eneiluj/cospend-nc</website>
    <bugs>https://gitlab.com/eneiluj/cospend-nc/issues</bugs>
    <screenshot>https://gitlab.com/eneiluj/cospend-nc/wikis/uploads/5e4d6f74e72560d9d7eeda3e3f62746f/1.png</screenshot>
    <screenshot>https://gitlab.com/eneiluj/cospend-nc/wikis/uploads/f4eef68a64dcbf89a59ea35a1cb21c79/2.png</screenshot>
    <screenshot>https://gitlab.com/eneiluj/cospend-nc/wikis/uploads/e36c1985a882b7d1b009735e5b8e71ae/3.png</screenshot>
    <dependencies>
        <database min-version="9.4">pgsql</database>
        <database>sqlite</database>
        <database min-version="5.5">mysql</database>
        <nextcloud min-version="17" max-version="19"/>
    </dependencies>
    <background-jobs>
        <job>OCA\Cospend\Cron\RepeatBills</job>
        <job>OCA\Cospend\Cron\AutoExport</job>
    </background-jobs>
    <commands>
        <command>OCA\Cospend\Command\RepeatBills</command>
        <command>OCA\Cospend\Command\ExportProject</command>
    </commands>
    <settings>
        <admin>OCA\Cospend\Settings\Admin</admin>
    </settings>
    <activity>
        <settings>
            <setting>OCA\Cospend\Activity\Setting</setting>
            <setting>OCA\Cospend\Activity\BillSetting</setting>
        </settings>
        <filters>
            <filter>OCA\Cospend\Activity\Filter</filter>
        </filters>
        <providers>
            <provider>OCA\Cospend\Activity\CospendProvider</provider>
        </providers>
    </activity>
</info>
